{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "variable-name": [
      true, "ban-keywords", "check-format", "allow-leading-underscore"
    ]
  }
}
